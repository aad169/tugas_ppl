import os
import numpy as np
import math

max = 4
A = np.zeros(shape=(max, max))
B = np.zeros(shape=(max))

A[0, 0] = 1.0
A[0, 1] = 2.0
A[1, 0] = 1.0
A[1, 1] = 1.0
A[1, 2] = 2.0
A[2, 1] = 1.0
A[2, 2] = 1.0
A[2, 3] = 2.0
A[3, 2] = 2.0
A[3, 3] = 1.0

B[0] = 5.0
B[1] = 7.0
B[2] = 10.0
B[3] = 7.0

C = np.dot(A, B)

print("Matrix A:")
print(A)
print()
print("Vector B:")
print(B)
print()
print("Result C=(A x B):")
print(C)
